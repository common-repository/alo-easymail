<!-- EasyMail tells to WMPL plugin that 'newsletter' post type has to be translated -->
<wpml-config>
    <custom-types>
        <custom-type translate="1">newsletter</custom-type>
    </custom-types>
    <admin-texts>
        <key name="blogname" />
        <key name="blogdescription" />
    </admin-texts>
</wpml-config>
